<h1>Sign Up Confirmation</h1>
<h3>Are you sure these values are correct?</h3>
<div class="row">
  <p>Email:
    <%= user.email %>
  </p>
  <p>Name:
    <%= user.name %>
  </p>
  <div class="form-group">
    <%= f.submit "Yes", class: 'btn btn-success' %>
  </div>
  <div class="form-goup">
    <%= f.submit "No", class: 'btn btn-cancel' %>
  </div>
</div>
